@*@model List<Student>*@
@model dynamic
@{
    ViewBag.Title = "Students";
}


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Personal Info</th>
            <th>Major</th>
            <th>Enrollment</th>
            <th></th>

        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model.Students)
        {
        <tr>
            <td>@c.StudentId</td>
            <td>@c.Name</td>

            <td>
                <a asp-controller="Student" asp-action="Details" asp-route-id="@c.StudentId" class="btn btn-info"> Details </a>
                <a asp-controller="Student" asp-action="Edit" asp-route-id=@c.StudentId class="btn btn-info"> Edit </a>

            </td>
            <td><a asp-controller="" asp-action="" asp-route-id="@c.StudentId" class="btn btn-info"> Details </a></td>
            <td><a asp-controller="" asp-action="" asp-route-id="@c.StudentId" class="btn btn-info"> Details </a></td>
            <td><a asp-controller="Student" asp-action="Delete" asp-route-id="@c.StudentId" class="btn btn-info"> Delete </a></td>
        </tr>
        }
    </tbody>
</table>
<form asp-action="Add" method="get">
    <button type="submit" class="btn btn-info"> Add Students </button>
</form>




